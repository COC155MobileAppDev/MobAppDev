<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobileapp.example.com.lecturerecorder" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mobileapp.example.com.lecturerecorder.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mobileapp.example.com.lecturerecorder.Section2"
            android:label="@string/title_activity_section2" >
        </activity>
        <activity
            android:name="mobileapp.example.com.lecturerecorder.MainActivity2"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name="mobileapp.example.com.lecturerecorder.ModulesActivity"
            android:label="@string/title_activity_modules" >
        </activity>
    </application>

</manifest>
